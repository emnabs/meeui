{
    "name": "@emhome/meeui",
    "version": "1.0.2",
    "description": "A Vue UI component library",
    "main": "lib/index.js",
    "module": "es/index.js",
    "style": "lib/index.css",
    "files": [
        "es",
        "lib",
        "types",
        "vetur"
    ],
    "npm": {
        "tag": "latest-v2"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "dev": "webpack-dev-server --config build/webpack.config.dev.js",
        "build": "webpack --config build/webpack.config.prod.js",
        "lint": "eslint src --ext .js,.vue"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "bugs": {
        "url": "https://github.com/emnabs/meeui/issues"
    },
    "lint-staged": {
        "*.md": "prettier --write",
        "*.{ts,tsx,js,vue,scss}": "prettier --write",
        "*.{ts,tsx,js,vue}": "eslint --fix",
        "*.{vue,css,scss}": "stylelint --fix"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:emnabs/meeui.git"
    },
    "keywords": [
        "vue",
        "ui",
        "components"
    ],
    "author": "youmbe",
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.28.4",
        "@vant/touch-emulator": "^1.4.0",
        "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
        "@vue/component-compiler-utils": "^2.6.0",
        "vue-lazyload": "^1.3.3"
    },
    "peerDependencies": {
        "vue": "^2.6.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "babel-eslint": "10.1.0",
        "babel-loader": "^7.1.5",
        "babel-plugin-component": "^1.1.1",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^2.1.0",
        "dayjs": "^1.11.18",
        "es6-promise": "^4.0.5",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^4.0.0",
        "mini-css-extract-plugin": "^0.4.1",
        "numeral": "^2.0.6",
        "prettier": "2.1.0",
        "sass": "1.26.2",
        "sass-loader": "8.0.2",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^5.3.14",
        "url-loader": "^4.1.0",
        "vue": "^2.6.12",
        "vue-loader": "^15.9.6",
        "vue-router": "^3.0.1",
        "vue-style-loader": "^4.1.3",
        "vue-template-compiler": "^2.6.14",
        "vue-template-es2015-compiler": "^1.6.0",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.11",
        "webpack-manifest-plugin": "^4.0.0",
        "webpack-node-externals": "^1.7.2"
    },
    "sideEffects": [
        "es/**/style/*",
        "lib/**/style/*",
        "*.css",
        "*.scss"
    ],
    "vetur": {
        "tags": "vetur/tags.json",
        "attributes": "vetur/attributes.json"
    },
    "web-types": "vetur/web-types.json",
    "eslintConfig": {
        "root": true,
        "extends": [
            "@emhome"
        ]
    },
    "prettier": {
        "singleQuote": true,
        "proseWrap": "never"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}