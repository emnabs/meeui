{
    "name": "@emhome/meeui",
    "version": "2.0.4",
    "description": "A Vue UI component library",
    "main": "lib/meeui.common.js",
    "module": "lib/meeui.esm.js",
    "unpkg": "lib/index.js",
    "jsdelivr": "lib/index.js",
    "style": "lib/theme-chalk/index.css",
    "typings": "types/index.d.ts",
    "files": [
        "lib",
        "src",
        "theme",
        "types",
        "vetur"
    ],
    "npm": {
        "tag": "latest-v2"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "dev": "webpack-dev-server --config build/webpack.config.dev.js",
        "build:file": "node build/build-entry.js && webpack --config build/webpack.common.js && webpack --config build/webpack.component.js",
        "build:theme": "node build/build-theme.js",
        "build:utils": "cross-env NODE_ENV=production webpack --config build/webpack.common.js --config build/webpack.component.js",
        "build": "npm run build:file && npm run build:theme",
        "lint": "eslint src --ext .js,.vue",
        "release": "npm version patch && npm run build && npm publish",
        "prepublishOnly": "npm run build"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "bugs": {
        "url": "https://github.com/emnabs/meeui/issues"
    },
    "lint-staged": {
        "*.md": "prettier --write",
        "*.{ts,tsx,js,vue,scss}": "prettier --write",
        "*.{ts,tsx,js,vue}": "eslint --fix",
        "*.{vue,css,scss}": "stylelint --fix"
    },
    "repository": {
        "type": "git",
        "url": "https://gitee.com/youmbe/meeui.git"
    },
    "keywords": [
        "vue",
        "ui",
        "components"
    ],
    "author": "youmbe",
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.28.4",
        "@vant/touch-emulator": "^1.4.0",
        "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
        "@vue/component-compiler-utils": "^2.6.0",
        "babel-eslint": "^10.1.0",
        "vue-lazyload": "^1.3.3"
    },
    "peerDependencies": {
        "vue": "^2.6.0"
    },
    "devDependencies": {
        "@babel/plugin-transform-object-rest-spread": "^7.28.4",
        "@babel/preset-env": "^7.28.3",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.0.0",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-component": "^1.1.1",
        "babel-plugin-istanbul": "^4.1.1",
        "babel-plugin-module-resolver": "^2.2.0",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-2": "^6.24.1",
        "babel-regenerator-runtime": "^6.5.0",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^2.1.0",
        "dayjs": "^1.11.18",
        "es6-promise": "^4.0.5",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^4.0.0",
        "mini-css-extract-plugin": "^0.4.1",
        "numeral": "^2.0.6",
        "prettier": "2.1.0",
        "progress-bar-webpack-plugin": "^2.1.0",
        "sass": "1.26.2",
        "sass-loader": "8.0.2",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^4.2.3",
        "url-loader": "^4.1.0",
        "vue": "^2.6.12",
        "vue-loader": "^15.9.6",
        "vue-router": "^3.0.1",
        "vue-style-loader": "^4.1.3",
        "vue-template-compiler": "^2.6.14",
        "vue-template-es2015-compiler": "^1.6.0",
        "webpack": "^4.14.0",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.11",
        "webpack-manifest-plugin": "^2.2.0",
        "webpack-node-externals": "^1.7.2"
    },
    "sideEffects": [
        "*.scss",
        "*.css",
        "lib/theme-chalk/*.css",
        "lib/theme-chalk/base.css",
        "src/**/style/*",
        "src/style/**"
    ],
    "vetur": {
        "tags": "vetur/tags.json",
        "attributes": "vetur/attributes.json"
    },
    "web-types": "vetur/web-types.json",
    "eslintConfig": {
        "root": true,
        "extends": [
            "@emhome"
        ]
    },
    "prettier": {
        "singleQuote": true,
        "proseWrap": "never"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
